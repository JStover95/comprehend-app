    "eslint": "^9.39.2",
    "jest": "^30.2.0",
    "prettier": "3.7.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.50.1"
  },
  "dependencies": {
    "aws-cdk-lib": "2.232.1",
    "constructs": "^10.0.0"
